            app:layout_constraintBottom_toTopOf="@+id/textView3"
            app:layout_constraintEnd_toEndOf="@+id/spinnerMeasureOld"
            app:layout_constraintHorizontal_bias="0.571"
            app:layout_constraintStart_toStartOf="@+id/spinnerMeasureOld"
            app:layout_constraintTop_toBottomOf="@+id/spinnerMeasureOld"
            app:layout_constraintVertical_bias="0.48"
