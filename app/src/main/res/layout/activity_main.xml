            app:layout_constraintBottom_toTopOf="@+id/textView9"
            app:layout_constraintEnd_toEndOf="@+id/spinnerMeasureCurrent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/spinnerMeasureCurrent"
            app:layout_constraintTop_toBottomOf="@+id/spinnerMeasureFuture" />

        <Spinner
            android:id="@+id/spinnerMeasureFuture"
            android:layout_width="91dp"
            android:layout_height="37dp"
